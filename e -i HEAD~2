[32m+[m[32m              height: 300,[m
[32m+[m[32m          });[m
[32m+[m[32m          chart.timeScale().fitContent();[m
[32m+[m
[32m+[m[32m          const newSeries = chart.addAreaSeries({ lineColor, topColor: areaTopColor, bottomColor: areaBottomColor });[m
[32m+[m[32m          newSeries.setData(data);[m
