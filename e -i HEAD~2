[32m+[m[32mexport const ChartComponent = props => {[m
[32m+[m[32m  const {[m
[32m+[m[32m      data,[m
[32m+[m[32m      colors: {[m
[32m+[m[32m          backgroundColor = 'white',[m
[32m+[m[32m          lineColor = '#2962FF',[m
[32m+[m[32m          textColor = 'black',[m
[32m+[m[32m          areaTopColor = '#2962FF',[m
[32m+[m[32m          areaBottomColor = 'rgba(41, 98, 255, 0.28)',[m
[32m+[m[32m      } = {},[m
