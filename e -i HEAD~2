[32m+[m[32m  useEffect([m
[32m+[m[32m      () => {[m
[32m+[m[32m          const handleResize = () => {[m
[32m+[m[32m              chart.applyOptions({ width: chartContainerRef.current.clientWidth });[m
[32m+[m[32m          };[m
[32m+[m
[32m+[m[32m          const chart = createChart(chartContainerRef.current, {[m
[32m+[m[32m              layout: {[m
[32m+[m[32m                  background: { type: ColorType.Solid, color: backgroundColor },[m
[32m+[m[32m                  textColor,[m
[32m+[m[32m              },[m
