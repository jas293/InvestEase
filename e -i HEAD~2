[32m+[m[32m          window.addEventListener('resize', handleResize);[m
[32m+[m
[32m+[m[32m          return () => {[m
[32m+[m[32m              window.removeEventListener('resize', handleResize);[m
[32m+[m
[32m+[m[32m              chart.remove();[m
[32m+[m[32m          };[m
[32m+[m[32m      },[m
